


# compressblockdevice config

[log]
log_file  = "/var/log/compressblockdevice/compressblockdevice.log"
log_level = 0

[catalog]
catalog_file = "/etc/compressblockdevice/catalog.toml"

[zosbd2]
control_device = "/dev/zosbd2ctl"

[compression_settings]
compression_level = 9
compression_type = "gzip"
